        "prerenderer",
        "spa",
        "ssr",
        "serverside-rendering",
        "puppeteer",
        "google-chrome",
        "headless-chrome",
        "weweb"
    ],
    "author": {
        "name": "Marc Fabre",
        "email": "marc@weweb.io",
        "url": "https://www.weweb.io/"
    },
    "contributors": [
        {
            "name": "Marc Fabre",
            "email": "marc@weweb.io",
            "url": "https://www.weweb.io/"
        },
        {
            "name": "Joshua Bemenderfer",
            "email": "tribex10@gmail.com",
            "url": "https://joshderf.com/"
        }
    ],
    "repository": "https://github.com/weweb-team/weweb-renderer-puppeteer",
    "bugs": {
        "url": "https://github.com/weweb-team/weweb-renderer-puppeteer/issues",
        "email": "contact@weweb.io"
    },
    "dependencies": {
        "promise-limit": "^2.5.0",
        "puppeteer": "^1.7.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.24.1",
        "eslint": "^2.5.1",
        "eslint-config-standard": "^5.1.0",
